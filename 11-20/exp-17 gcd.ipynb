    "a=int(input(\"enter first no:\"))\n",
    "b=int(input(\"enter second no:\"))\n",
    "i=1\n",
    "while(i<=a and i<=b):\n",
    "    if(a%i==0 and b%i==0):\n",
    "        gcd=i\n",
    "    i=i+1\n",
    "print(\"gcd of\",a,\"and\",b,\"is:\",gcd)"
